{
  "multipart": [
    { "when":
      { "OR": [
        { "north": "none", "east": "none", "south": "none", "west": "none" },
        { "north": "side|up", "east":  "side|up" },
        { "east":  "side|up", "south": "side|up" },
        { "south": "side|up", "west":  "side|up" },
        { "west":  "side|up", "north": "side|up" }
      ] },
      "apply": { "model": "minecraft:block/redstone_dust_dot" }
    },
    { "when":
      { "OR": [
        { "north": "none", "east": "none", "south": "side|up", "west": "none" },
        { "north": "side|up" }
      ] },
      "apply": { "model": "minecraft:block/redstone_dust_side0" }
    },
    { "when":
      { "OR": [
        { "north": "side|up", "east": "none", "south": "none", "west": "none" },
        { "south": "side|up" }
      ] },
      "apply": { "model": "minecraft:block/redstone_dust_side_alt0" }
    },
    { "when":
      { "OR": [
        { "north": "none", "east": "none", "south": "none", "west": "side|up" },
        { "east": "side|up" }
      ] },
      "apply": { "model": "minecraft:block/redstone_dust_side_alt1", "y": 270 }
    },
    { "when":
      { "OR": [
        { "north": "none", "east": "side|up", "south": "none", "west": "none" },
        { "west": "side|up" }
      ] },
      "apply": { "model": "minecraft:block/redstone_dust_side1", "y": 270 }
    },

    { "when": { "north": "up" }, "apply": { "model": "minecraft:block/redstone_dust_up"           } },
    { "when": { "east":  "up" }, "apply": { "model": "minecraft:block/redstone_dust_up", "y":  90 } },
    { "when": { "south": "up" }, "apply": { "model": "minecraft:block/redstone_dust_up", "y": 180 } },
    { "when": { "west":  "up" }, "apply": { "model": "minecraft:block/redstone_dust_up", "y": 270 } },

    { "when": { "power":  0 }, "apply": { "model": "minecraft:block/rs00" } },
    { "when": { "power":  1 }, "apply": { "model": "minecraft:block/rs01" } },
    { "when": { "power":  2 }, "apply": { "model": "minecraft:block/rs02" } },
    { "when": { "power":  3 }, "apply": { "model": "minecraft:block/rs03" } },
    { "when": { "power":  4 }, "apply": { "model": "minecraft:block/rs04" } },
    { "when": { "power":  5 }, "apply": { "model": "minecraft:block/rs05" } },
    { "when": { "power":  6 }, "apply": { "model": "minecraft:block/rs06" } },
    { "when": { "power":  7 }, "apply": { "model": "minecraft:block/rs07" } },
    { "when": { "power":  8 }, "apply": { "model": "minecraft:block/rs08" } },
    { "when": { "power":  9 }, "apply": { "model": "minecraft:block/rs09" } },
    { "when": { "power": 10 }, "apply": { "model": "minecraft:block/rs10" } },
    { "when": { "power": 11 }, "apply": { "model": "minecraft:block/rs11" } },
    { "when": { "power": 12 }, "apply": { "model": "minecraft:block/rs12" } },
    { "when": { "power": 13 }, "apply": { "model": "minecraft:block/rs13" } },
    { "when": { "power": 14 }, "apply": { "model": "minecraft:block/rs14" } },
    { "when": { "power": 15 }, "apply": { "model": "minecraft:block/rs15" } }
  ]
}
